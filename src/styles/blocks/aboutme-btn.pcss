.aboutme__buttn-group {
    display: flex;
}

.aboutme__buttons {
    width: 50px;
    height: 50px;
    border: 1px solid #fff;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-right: 30px;
    background: transparent;

    &-prev {
    transform: rotate(180deg);
    }

    &:hover{
        background:#ff9a00;
        transition: 0.4s
        }
}


.aboutme__buttons-icon {
    width: 8px;
    height: 10px;
    fill: #fff;
}
