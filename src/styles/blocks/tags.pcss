.tags{
    display: flex;
}
.tags__item{
    padding: 18px 20px;
    background: #f3f5f9;
    border-radius: 30px;
    font-size: 15px;
    font-weight: 600;
    text-align: center;

    @include tablets {
        margin-right: 30px;
        &:last-child{
            margin-right: 0px;
        }
    }
}